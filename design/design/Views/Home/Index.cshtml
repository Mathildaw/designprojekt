@{
    ViewBag.Title = "Home Page";
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
    <div style=" text-align: center;"><h1>Startsida</h1></div>
    

    <div class="row">
        <div class="col-md-6">
            <h2 style="text-align:center;">För folk som tycker om att bo!</h2>
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Carousel indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>
                <!-- Wrapper for carousel items -->
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="~/Content/Images/The-Shoals-Course-1613273.jpg" alt="First Slide">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/the-shoals-course-1613272_960_720.jpg" alt="Second Slide">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/Golf_ball_grass.jpg" alt="Third Slide">
                    </div>
                </div>
                <!-- Carousel controls -->
                <a class="carousel-control left" href="#myCarousel" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
                <a class="carousel-control right" href="#myCarousel" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
            <p>Hyr din bostad</p>
            <ul>
                <li>Trygghet</li>
                <li>Frihet</li>
                <li>Service</li>
            </ul>
            <p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301865">Learn more &raquo;</a></p>
        </div>
        <div class="col-md-6">
            <h2 style="text-align: center;">Get more libraries</h2>
            <p>NuGet is a free Visual Studio extension that makes it easy to add, remove, and update libraries and tools in Visual Studio projects.</p>
            <p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301866">Learn more &raquo;</a></p>
        </div>
      
    </div>
